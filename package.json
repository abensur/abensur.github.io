  "dependencies": {
    "nuxt": "^4.3.0",
    "vue": "^3.5.27",
    "vue-router": "^5.0.2"
  },
  "devDependencies": {
    "@iconify-json/lucide": "^1.2.88",
    "@nuxt/icon": "^2.2.1",
    "@nuxt/types": "^2.18.1",
    "typescript": "^5.9.3"
  }
}
